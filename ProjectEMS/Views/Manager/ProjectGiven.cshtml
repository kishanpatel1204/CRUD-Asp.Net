@model ProjectEMS.Models.Emp_projRequestModel

@{
    ViewData["Title"] = "Assign Project ";
}
<div class="row">
    <div class="col-md-4">
        <form asp-action="ProjectGiven">
            <div class="form-group">
                <label asp-for="P_id" class="control-label"></label>
               
                <select asp-for="P_id" class="form-control" asp-items="(IEnumerable<SelectListItem>)ViewBag.listproject">
                </select>
            </div>
            <div class="form-group">
                <label asp-for="E_id" class="control-label"></label>
               <select asp-for="E_id" class="form-control" asp-items="(IEnumerable<SelectListItem>)ViewBag.listemployees">
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="projectList">Back to List</a>
</div>